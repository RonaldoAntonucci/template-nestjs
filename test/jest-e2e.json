{
    "moduleFileExtensions": ["ts", "js"],
    "rootDir": ".",
    "testEnvironment": "node",
    "testRegex": ".e2e-spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "moduleNameMapper": {
      "^src/(.*)$": "<rootDir>/../src/$1",
      "^test/(.*)$": "<rootDir>/$1"
    },
    "coverageDirectory": "../coverage_e2e",
    "collectCoverageFrom": [
      "/../src/**/*.ts",
      "!/../src/main.ts"
    ]
  }
  