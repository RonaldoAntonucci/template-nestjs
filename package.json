{
  "name": "template-api",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start -w",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "vitest run",
    "test:staged": "vitest run --passWithNoTests",
    "test:watch": "vitest",
    "test:cov": "npm run test -- --coverage",
    "test:ui": "vitest --ui --coverage --coverage.reporter=html",
    "test:debug": "vitest --inspect-brk --inspect --logHeapUsage --threads=false",
    "test:e2e": "docker compose exec api npx vitest run --config ./test./vitest-e2e.config.ts",
    "test:e2e:cov": "docker compose exec api npm run test:e2e -- --coverage",
    "prepare": "node .husky/install.mjs | true",
    "typeorm": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js",
    "migration:generate": "npm run typeorm -- migration:generate src/shared/database/migrations/migrations -d datasource.dev.ts",
    "migration:run": "npm run typeorm -- migration:run -d datasource.dev.ts",
    "migration:revert": "npm run typeorm -- migration:revert -d datasource.dev.ts"
  },
  "dependencies": {
    "@nestjs/common": "^10.4.6",
    "@nestjs/config": "^3.3.0",
    "@nestjs/core": "^10.4.6",
    "@nestjs/jwt": "^10.2.0",
    "@nestjs/platform-express": "^10.4.6",
    "@nestjs/swagger": "^7.4.2",
    "@nestjs/terminus": "^10.2.3",
    "argon2": "^0.41.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "pg": "^8.13.1",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.1",
    "uuid": "^11.0.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.5.0",
    "@commitlint/config-conventional": "^19.5.0",
    "@eslint/js": "^9.13.0",
    "@nestjs/cli": "^10.4.5",
    "@nestjs/schematics": "^10.2.2",
    "@nestjs/testing": "^10.4.6",
    "@swc/cli": "^0.4.0",
    "@swc/core": "^1.7.40",
    "@types/express": "^5.0.0",
    "@types/node": "^22.8.1",
    "@types/supertest": "^6.0.2",
    "@types/uuid": "^10.0.0",
    "@vitest/coverage-v8": "^2.1.3",
    "@vitest/ui": "^2.1.3",
    "eslint": "^9.13.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "globals": "^15.11.0",
    "husky": "^9.1.6",
    "lint-staged": "^15.2.10",
    "prettier": "3.3.3",
    "source-map-support": "^0.5.21",
    "supertest": "^7.0.0",
    "ts-loader": "^9.5.1",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.6.3",
    "typescript-eslint": "^8.11.0",
    "unplugin-swc": "^1.5.1",
    "vite-tsconfig-paths": "^5.0.1",
    "vitest": "^2.1.3"
  }
}